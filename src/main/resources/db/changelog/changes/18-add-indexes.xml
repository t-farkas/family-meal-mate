<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="index-household-joinid" author="t-farkas">
        <addUniqueConstraint
                tableName="household"
                columnNames="join_id"
                constraintName="uk_household_join_id"/>
    </changeSet>

    <changeSet id="index-user-email" author="t-farkas">
        <addUniqueConstraint
                tableName="users"
                columnNames="email"
                constraintName="uk_user_email"/>
    </changeSet>

</databaseChangeLog>